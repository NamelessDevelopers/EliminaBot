# Elimina Bot Configuration
# Copy this to .env and fill in your values

# Discord bot token from https://discord.com/developers/applications
BOT_TOKEN=your_bot_token_here

# SQLite database path (or any SQLAlchemy-compatible URI)
DB_URI=sqlite+aiosqlite:///elimina.db

# Support info shown in /info command
SUPPORT_EMAIL=your@email.com
SUPPORT_SERVER_INVITE=https://discord.gg/your_invite
SUPPORT_SERVER_ID=123456789012345678

# Channel ID for join/leave logging (in your support server)
JOIN_LEAVE_CHANNEL=123456789012345678

# Default command prefix
BOT_PREFIX=e!

# GitHub repo URL
GITHUB_URL=https://github.com/namelessdevelopers/eliminabot

# Top.gg bot ID (for voting links)
TOP_GG_ID=123456789012345678

# Poll reaction emotes (emoji or custom emote IDs)
